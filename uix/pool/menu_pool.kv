#:import hex kivy.utils.get_color_from_hex

<MenuPool>:
    orientation: 'vertical'
    canvas:
        Color:
            rgba: hex("#726D64")
        Rectangle:
            pos: self.pos
            size: self.size

    id: bottom_navigation
    size_hint_y: 0.12
    size_hint_x: 1
    pos_hint:{'bottom':1}
